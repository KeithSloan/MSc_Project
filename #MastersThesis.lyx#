reference "tab:uminusr---Environmental"
plural "false"
caps "false"
noprefix "false"

\end_inset

 and also the log of the same measures, figure 
\begin_inset CommandInset ref
LatexCommand ref
reference "fig:Density-Plots-for"
plural "false"
caps "false"
noprefix "false"

\end_inset

 and table 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:uminusr---Log(Environmnetal"
plural "false"
caps "false"
noprefix "false"

\end_inset


\end_layout

